<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 用户角色 -->
<hibernate-mapping auto-import="false">  
    <class name="com.poweruniverse.nim.data.entity.YongHuJS" table="SYS_YongHuJS" >
    	<!--主键-->
        <id name="yongHuJSDM" type="integer">
	        <column name="yongHuJSDM" precision="12" scale="0" />
	        <generator class="increment"></generator>
        </id>
        
		<!--角色-->
        <many-to-one name="jueSe" class="com.poweruniverse.nim.data.entity.JueSe" fetch="select">
            <column name="jueSeDM" precision="12" scale="0" not-null="false" />
        </many-to-one>
        
		<!--用户-->
        <many-to-one name="yongHu" class="com.poweruniverse.nim.data.entity.YongHu" fetch="select">
            <column name="yongHuDM" precision="12" scale="0" not-null="false" />
        </many-to-one>
        
     </class>
</hibernate-mapping>
