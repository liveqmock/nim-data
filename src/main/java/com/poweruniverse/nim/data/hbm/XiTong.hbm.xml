<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- version:2015-01-20 18:30:34 -->
<hibernate-mapping auto-import="false">  
    <class name="com.poweruniverse.nim.data.entity.XiTong" table="SYS_XiTong" >
    	<!--主键-->
        <id name="xiTongDM" type="integer">
	        <column name="xiTongDM" precision="12" scale="0" />
	        <generator class="increment"></generator>
        </id>
        
		<!--系统名称-->
        <property name="xiTongMC" type="java.lang.String" lazy="false">
        	<column name="xiTongMC" precision="200" scale="0" not-null="false" />
        </property>
		<!--系统代号-->
        <property name="xiTongDH" type="java.lang.String" lazy="false">
        	<column name="xiTongDH" precision="200" scale="0" not-null="false" />
        </property>
		<!--系统首页-->
        <property name="xiTongSY" type="java.lang.String" lazy="false">
        	<column name="xiTongSY" precision="200" scale="0" not-null="false" />
        </property>
		<!--是否问题-->
        <property name="shiFouWT" type="java.lang.Boolean" lazy="false">
        	<column name="shiFouWT" precision="1" scale="0" not-null="false" />
        </property>
		<!--需要登录-->
        <property name="xuYaoDL" type="java.lang.Boolean" lazy="false">
        	<column name="xuYaoDL" precision="2" scale="0" not-null="false" />
        </property>
		<!--form类型-->
        <property name="formType" type="java.lang.String" lazy="false">
        	<column name="formType" precision="200" scale="0" not-null="false" />
        </property>
		<!--是否历史-->
        <property name="shiFouLS" type="java.lang.Boolean" lazy="false">
        	<column name="shiFouLS" precision="2" scale="0" not-null="false" />
        </property>
		<!--登录叶面-->
        <property name="dengLuYM" type="java.lang.String" lazy="false">
        	<column name="dengLuYM" precision="200" scale="0" not-null="false" />
        </property>
		<!--后台页面-->
        <property name="houTaiYM" type="java.lang.String" lazy="false">
        	<column name="houTaiYM" precision="200" scale="0" not-null="false" />
        </property>
		<!--是否使用验证码-->
        <property name="shiFouSYYZM" type="java.lang.Boolean" lazy="false">
        	<column name="shiFouSYYZM" precision="2" scale="0" not-null="false" />
        </property>
     </class>
</hibernate-mapping>
