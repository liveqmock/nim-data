<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 用户状态 -->
<hibernate-mapping auto-import="false">  
    <class name="com.poweruniverse.nim.data.entity.YongHuZT" table="SYS_YongHuZT" >
    	<!--主键-->
        <id name="yongHuZTDM" type="integer">
	        <column name="yongHuZTDM" precision="12" scale="0" />
	        <generator class="increment"></generator>
        </id>
        
		<!--用户状态名称-->
        <property name="yongHuZTMC" type="java.lang.String" lazy="false">
        	<column name="yongHuZTMC" precision="200" scale="0" not-null="false" />
        </property>
     </class>
</hibernate-mapping>
